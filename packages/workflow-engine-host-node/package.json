{
  "name": "@bratsos/workflow-engine-host-node",
  "version": "0.2.2",
  "description": "Node.js host for @bratsos/workflow-engine command kernel",
  "type": "module",
  "license": "MIT",
  "author": "Alex Bratsos",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bratsos/workflow-engine": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22",
    "tsup": "^8.5.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "zod": "^4.1.12"
  },
  "engines": {
    "node": ">=22.11.0"
  }
}
